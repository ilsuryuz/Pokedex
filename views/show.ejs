
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title><%=data.name%></title>
  </head>
  <body>
    <div>
        <div>
            <h2>Pokedex Entry - <%=data.name%></h2>
            <form action="/Pokedex">
                <input type="submit" value="Back to Pokedex" />
              </form>
        </div>
        <div>
            <h3><%= data.name%></h3>
            <img src="<%=data.img%>" alt="<%=data.name%>">
            <h4>ID # <%=data.id%></h4>
            <h5><%=data.misc.classification%></h5>
            <form action="" method="">
                <input type="submit" value="Edit Pokemen" />
              </form>
              <div>
                <h4>Type</h4>
                <ul>
                    <% for(let i = 0; i < data.type.length; i++) { %>
                    <li>
                      <%=data.type[i]%>
                    </li>
                    <% } %>
                  </ul>
              </div>
        </div>
        <div>
            <h4>Stats</h4>
            <ul>
            <% for (const property in data.stats) { %>
                <li type="none">
                    <%=property[0].toUpperCase() + property.slice(1, property.length)%>: <%=data.stats[property]%>
                </li>
                <% } %>
            </ul>
        </div>

        <div>
            <h4>Damages</h4>
            <ul>
                <% for (const property in data.damages) { %>
                    <li type="none">
                        <%=property[0].toUpperCase() + property.slice(1, property.length)%>: <%=data.damages[property]%>
                    </li>
                    <% } %>
                </ul>
        </div>
    </div>
  </body>
</html>

